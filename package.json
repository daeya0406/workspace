{
  "name": "daeya",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore src",
    "lint:fix": "eslint --ext .js,.vue --ignore-path .gitignore --fix src",
    "format": "prettier --write \"src/**/*.{js,vue,scss,json}\"",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "dependencies": {
    "axios": "^1.8.3",
    "core-js": "^3.8.3",
    "pinia": "^3.0.1",
    "vue": "^3.2.13",
    "vue-router": "4",
    "vuedraggable": "^2.24.3"
  },
  "devDependencies": {
    "@babel/core": "^7.12.16",
    "@babel/eslint-parser": "^7.12.16",
    "@chromatic-com/storybook": "^3",
    "@storybook/addon-essentials": "^8.6.9",
    "@storybook/addon-interactions": "^8.6.9",
    "@storybook/addon-onboarding": "^8.6.9",
    "@storybook/addon-webpack5-compiler-swc": "^3.0.0",
    "@storybook/blocks": "^8.6.9",
    "@storybook/test": "^8.6.9",
    "@storybook/vue3": "^8.6.9",
    "@storybook/vue3-webpack5": "^8.6.9",
    "@vitest/browser": "^3.0.9",
    "@vitest/coverage-v8": "^3.0.9",
    "@vue/cli-plugin-babel": "~5.0.0",
    "@vue/cli-plugin-eslint": "~5.0.0",
    "@vue/cli-service": "~5.0.0",
    "@vue/compiler-sfc": "^3.2.0",
    "@vue/eslint-config-prettier": "^10.2.0",
    "eslint": "^8.21.0",
    "eslint-config-prettier": "^10.1.2",
    "eslint-plugin-prettier": "^5.4.0",
    "eslint-plugin-storybook": "^0.12.0",
    "eslint-plugin-vue": "^8.0.3",
    "playwright": "^1.51.1",
    "prettier": "^3.5.3",
    "sass": "^1.85.1",
    "sass-loader": "10",
    "storybook": "^8.6.9",
    "vitest": "^3.0.9",
    "vue-loader": "^17.0.0",
    "webpack": "^5.0.0"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true,
      "vue/setup-compiler-macros": true
    },
    "extends": [
      "plugin:vue/vue3-essential",
      "eslint:recommended",
      "plugin:storybook/recommended",
      "@vue/prettier"
    ],
    "parserOptions": {
      "parser": "@babel/eslint-parser"
    },
    "rules": {
      "no-console": "off",
      "no-debugger": "off",
      "vue/multi-word-component-names": "off",
      "quotes": [
        "error",
        "single",
        {
          "avoidEscape": true
        }
      ]
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead",
    "not ie 11"
  ]
}
